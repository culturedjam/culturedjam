{
  "name": "culturedjam-gatsby",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "gatsby develop -p 3000 -o",
    "develop": "yarn clean && yarn start",
    "build": "gatsby build",
    "serve": "gatsby serve -o",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@fontsource/jura": "^4.5.10",
    "@fontsource/kanit": "^4.5.8",
    "@fontsource/major-mono-display": "^4.5.9",
    "@vanilla-extract/css-utils": "^0.1.2",
    "gatsby": "^4.17.2",
    "gatsby-plugin-image": "^2.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/eslint-plugin": "^11.7.0",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
